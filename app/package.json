{
  "name": "logalyzer-icecast",
  "version": "1.0.0",
  "description": "Icecast Log Analyzer.",
  "main": "logalyze.js",
  "scripts": {
    "build": "npm run build-entities && npm run build-svelte",
    "build-entities": "babel -d build/ entities/",
    "build-svelte": "npm run build-LogalyzerApp",
    "build-LogalyzerApp": "svelte compile --format es src/LogalyzerApp.html > build/LogalyzerApp.js && babel build/LogalyzerApp.js -o build/LogalyzerApp.js",
    "watch-entities": "babel --watch -d build/ entities/",
    "prepare": "npm run build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "icecast",
    "log",
    "logs",
    "analyze",
    "analyzer"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sciactive/logalyzer-icecast.git"
  },
  "author": "Hunter Perrin <hperrin@gmail.com>",
  "license": "Apache-2.0",
  "dependencies": {
    "chart.js": "^2.7.1",
    "chartjs-plugin-zoom": "^0.5.0",
    "compressing": "^1.2.3",
    "curl": "^0.1.4",
    "minimist": "^1.2.0",
    "moment": "^2.19.2",
    "n-readlines": "^0.2.8",
    "nymph-client": "^3.0.1",
    "nymph-client-node": "^2.0.0",
    "nymph-query-editor": "^1.0.0",
    "tilmeld": "^1.0.0-beta.7",
    "ua-parser-js": "^0.7.17"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-es2015-modules-umd": "^6.24.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-3": "^6.24.1",
    "babel-watch": "^2.0.7",
    "svelte-cli": "^1.4.0"
  }
}
